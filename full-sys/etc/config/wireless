
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'soc/soc:pcie/pci0000:00/0000:00:01.0/0000:01:00.0'
	option channel 'auto'
	option band '5g'
	option htmode 'VHT80'
	option country 'US'
	option cell_density '3'
	option txpower '22'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan lan_6'
	option mode 'ap'
	option ssid 'thatSweetTouch'
	option encryption 'sae-mixed'
	option macaddr 'c0:56:27:c7:c2:e8'
	option key 'playinthedirt'
	option ieee80211r '1'
	option ft_over_ds '0'
	option disassoc_low_ack '0'
	option ocv '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'soc/soc:pcie/pci0000:00/0000:00:02.0/0000:02:00.0'
	option channel 'auto'
	option band '2g'
	option htmode 'HT20'
	option country 'US'
	option cell_density '0'
	option disabled '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'thatSweetTouch'
	option encryption 'sae-mixed'
	option macaddr 'c0:56:27:c7:c2:e7'
	option key 'playinthedirt'
	option ieee80211r '1'
	option ft_over_ds '0'
	option disassoc_low_ack '0'
	option disabled '1'

config wifi-iface 'wifinet2'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Studio 504 (Guest)'
	option encryption 'none'
	option isolate '1'
	option disassoc_low_ack '0'
	option network 'lan'
	option disabled '1'

config wifi-iface 'wifinet3'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Studio 504 (Guest)'
	option encryption 'none'
	option disassoc_low_ack '0'
	option network 'lan'
	option disabled '1'

config wifi-iface 'wifinet4'
	option device 'radio0'
	option mode 'ap'
	option ssid 'linksys-rap-test'
	option encryption 'sae'
	option network 'lan lan_6'
	option disassoc_low_ack '0'
	option key 'faggot26'
	option ocv '0'

